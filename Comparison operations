a=int(input ('enter a value'))
b=int(input('enter b value'))
if a<b:
print('true')
else 
print('false')
if a>b:
print('true')
else 
print('false')
if a==b
print('true')
else
Print('false')
if a!=b
print('true')
else
Print('false')
if a<=b
print ('true')
else
Print ('false')
if a>=b
print ('true')
else
Print ('false')

